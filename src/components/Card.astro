---
export interface Props {
    url: string;
    ranking: string;
    searchTerm: string;
}

const { url, ranking, searchTerm } = Astro.props as Props;
---

<div class="card">
    <h2>{url}</h2>
    <p>{ranking} for {searchTerm}</p>
</div>

<style>
    .card {
        background: #fff;
        padding: 20px;
        border-radius: 10px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        margin-bottom: 20px;
    }
</style>